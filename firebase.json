{
 "firestore": {
    "rules": "firestore.rules",
     "indexes": "firestore.indexes.json"
   },
  "hosting": {
    "predeploy": [
      "ng build"
    ],
    "public": "dist/invoice-frontend2",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  } ,
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": "functions"
  }
}
